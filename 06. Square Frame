function drawSquareFrame(n) {
    //print top
    let topRow = "+";
    for(var top = 0;top < n - 2;top++){
    topRow = topRow + " -";
    }
    topRow = topRow + " +";
    console.log(topRow); 
    //print middle
    for(let mid = 0; mid < n - 2;mid++){
        let middleRow = "|";
        for(let j = 0;j < n - 2;j++){
            middleRow = middleRow + " -";
        }
        middleRow = middleRow + " |";
        console.log(middleRow);
    }
    //print bottom
    let botRow = "+";
    for(var bot = 0;bot < n - 2;bot++){
    botRow = botRow + " -";
    }
    botRow = botRow + " +";
    console.log(botRow);  
}
drawSquareFrame(5)
